
<br/>

<h3>Youtube IDs</h3>
<table class="table table-striped table-bordered table-condensed">
  <thead>
    <tr>
      <th>Id</th>
      <th>Name</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
<% @youtube_ids.each do |youtube_id| %>
  <tr>
    <td><%= youtube_id.y_id %></td>
    <td><%= youtube_id.name %></td>
    <td><%= link_to "Delete", youtube_id, :data => { :confirm => 'Are you sure?' }, :method => :delete %></td>
  </tr>
  </tbody>
<% end %>
</table>

<%= link_to 'New Youtube', new_youtube_id_path, :class => "btn"  %>

<br />